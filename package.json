{
  "name": "grocerrific",
  "version": "1.0.0",
  "description": "A grocery shopping full stack application built with React and Express",
  "main": "dist/server.js",
  "repository": "https://github.com/monsieurkayode/grocerrific.git",
  "author": "Kayode Adeola <monsieurkayode@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js --open",
    "start": "node dist/server/server.js",
    "start:dev": "nodemon dist/server/server.js",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "build:client:dev": "webpack --config webpack.config.dev.js --watch",
    "build:client:prod": "webpack --config webpack.config.prod.js",
    "prebuild:dev": "rm -rf dist",
    "prebuild:prod": "rm -rf dist",
    "build:server": "babel --presets env shared -d dist/shared/ && babel --presets env server -d dist/server/",
    "build:dev": "concurrently \"yarn build:client:dev\" \"yarn build:server\"",
    "build:prod": "concurrently \"yarn build:client:prod\" \"yarn build:server\"",
    "heroku-postbuild": "yarn build:prod",
    "test:dev": "jest --config jest.config.json -u --coverage",
    "test:watch": "yarn test:dev --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.18.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mongoose": "^5.3.11",
    "morgan": "^1.9.1",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-preloader-icon": "^0.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.4.3",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "webpack": "^4.20.2",
    "webpack-merge": "^4.1.4"
  }
}
